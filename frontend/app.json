{
  "expo": {
    "name": "coolchop",
    "slug": "coolchop",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/coolchop.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/coolchop.png",
      "resizeMode": "contain",
      "backgroundColor": "#DC2C10;"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We need your location to show you nearby services"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/coolchop.png",
        "backgroundColor": "#DC2C10"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/coolchop.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
